    <string-array name="learningMethod">
        <item>Select</item>
        <item>Self Study</item>
        <item>Discussion</item>
        <item>Consultation</item>
    </string-array>

    <string-array name="studyLocation">
        <item>Select</item>
        <item>Library</item>
        <item>Home</item>
        <item>Open Area</item>
        <item>Cafe</item>
        <item>Others</item>
